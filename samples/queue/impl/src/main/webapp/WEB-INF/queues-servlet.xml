<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
              http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="messageFactory" class="org.springframework.ws.soap.saaj.SaajSoapMessageFactory"/>

   <bean id="messageReceiver" class="org.globus.resourceful.springws.ResourcefulEndpointFactory">
        <property name="address" value="http://localhost:8080/queue/queues"/>
        <property name="serviceObject" ref="queueService"/>
        <property name="jaxbContextPath" value="org.globus.queueservice"/>
        <property name="beforeInterceptors">
            <list/>
        </property>
    </bean>

    <bean id="queueService" class="org.globus.queue.DefaultQueueService"/>
</beans>